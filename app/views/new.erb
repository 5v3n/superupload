<html>
<head>
  <title>Super Upload</title>
  <meta content="text/html;charset=utf-8" http-equiv="Content-Type">
  <link href="/stylesheets/style.css" rel="stylesheet" type="text/css" />
</head>
<body>
  <h1>SuperUpload</h1>
  <form action="/uploads" method="post" enctype="multipart/form-data" class="js-upload-form" id="file-upload-form">
    <input type="file" name="file" class="js-file-upload-input">
  </form>
  <iframe class=".js-upload-target" name="upload-target" src="" style="width:0;height:0;border:0px solid #fff;"></iframe>
  <div class="js-status">
  </div >
  <form action="/comments" method="post" enctype="multipart/form-data" class="js-comment-form">
    <textarea name="comment" class="js-comment-input"></textarea>
    <input type="submit" value="Save" class="js-comment-submit">
  </form>
<script src="/javascripts/lib/jquery.min.js"></script>
<script src="/javascripts/application.js"></script>
</body>
</html>